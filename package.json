{
  "name": "lei.github.io",
  "version": "1.0.0",
  "description": "GitHub User Pages",
  "dependencies": {
    "firebase": "^4.0.0",
    "normalize.css": "^7.0.0",
    "nuxt": "^0.10.7",
    "promise-polyfill": "^6.0.2",
    "vue-i18n": "^6.1.3",
    "vuefire": "^1.4.2",
    "vuetify": "^0.11.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "css-loader": "^0.28.1",
    "style-loader": "^0.17.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build-check": "nuxt build --analyze",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "pull": "git subtree pull --prefix dist origin master",
    "push": "git subtree push --prefix dist origin master",
    "commit": "git add . && git commit -am 'deploy'",
    "deploy": "npm run generate && npm run commit && npm run push",
    "surge": "surge --domain gh.lei.sh dist",
    "lint": "eslint --ext .js,.vue --ignore-pattern dist/_nuxt --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lei/lei.github.io.git"
  },
  "author": "LEI",
  "bugs": {
    "url": "https://github.com/LEI/lei.github.io/issues"
  },
  "homepage": "https://github.com/LEI/lei.github.io#readme"
}
